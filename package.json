{
  "name": "@ircam/devicemotion",
  "version": "1.1.0",
  "description": "project starter for libraries",
  "main": "dist/devicemotion.js",
  "author": "Benjamin Matuszewski",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -Rf dist",
    "deploy": "np --yolo",
    "doc": "jsdoc -c jsdoc.json",
    "version": "npm run clean && npm run transpile && npm run doc && git add docs",
    "test": "babel-tape-runner tests/",
    "transpile": "babel src --out-dir dist",
    "watch": "npm run clean && npm run transpile && chokidar src -c \"npm run transpile\""
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "platform-detect": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@ircam/jsdoc-template": "^1.0.3",
    "babel-tape-runner": "^3.0.0",
    "chokidar": "^3.2.2",
    "chokidar-cli": "^2.0.0",
    "concurrently": "^5.0.0",
    "jsdoc": "^3.6.3",
    "np": "^5.1.1",
    "tape": "^4.11.0"
  }
}
